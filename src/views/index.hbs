<div class="container-fluid px-0">
  <div class="row mx-0 my-1">
    <div class="col-md-9">
    <div class="card-body">
      <div class="row">
        {{#each images}}
            <div class="col-md-4 ">
            <div class="card mb-4 box-shadow bg-secondary">
              <a class="text-decoration-none" href="/images/{{uniqueId}}">
              <img
                src="/public/upload/{{filename}}"
                class="card-img-top"
                alt="Card image cap"
              />
              <div class="card-body">
                <p class="card-text">{{title}}</p>
                <p class="card-text">{{description}}</p>
                <div class="d-flex justify-content-between align-items-center">
                  <div class="btn-group">
                    <button
                      type="button"
                      class="btn btn-sm btn-outline-secondary text-white"
                    >View</button>
                    <button
                      type="button"
                      class="btn btn-sm btn-outline-secondary text-white"
                    >Edit</button>
                  </div>
                  <small class="text-muted">9 mins</small>
                </div>
              </div>
              </a>
            </div>
          </div>
        {{/each}}
      </div>
    </div>
  </div>
    <div class="col-md-3">
      {{>stats}}
      {{>popular}}
      {{>comments}}
    </div>
  </div>
</div>